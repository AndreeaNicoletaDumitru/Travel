@model Travel.ViewModels.ContactViewModel
@{
    ViewBag.Title = "Contact Page";
}
<script src="~/lib/jquery/wwwroot/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery/wwwroot/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
<h2>Contact me</h2>

<form>
    <label asp-for="Name">Name</label>
    <input asp-for="Name"/>
    <label asp-for="Email">Email</label>
    <input type="email" asp-for="Email"/>
    <label asp-for="Message">Message</label>
    <textarea cols="40" rows="4" asp-for="Message"></textarea>
    <div>
        <input type="submit" value="Send Message" />
    </div>
</form>